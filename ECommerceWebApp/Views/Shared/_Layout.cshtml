<body class="bg-body">
    <div id="shopify-section-header" class="shopify-section index-section">
        <header id="header" class="header-v2 delay05 fixed">
            <div class="container">
                <div class="row flex">
                    <!--========== SEARCH BAR STARTS ==========-->
                    <div class="col-lg-3 col-md-3 hidden-sm hidden-xs">
                        <a href="#" class="delay03 inline-block hidden" id="btn-search">
                            <i class="icon-magnifier fz-18"></i>
                        </a>
                        <form asp-controller="Products" asp-action="Index" class="searchform relative">
                            <label class="sr-only">search</label>
                            <input type="hidden" name="type" value="product">
                            <input name="SearchString" type="text" autocomplete="off" placeholder="ENTER KEYWORDS..." aria-label="I&#39;m looking for..." />
                            <button type="submit" class="submit absolute"><i class="icon-magnifier fz-16" aria-hidden="true"></i></button>
                        </form>
                    </div>
                    <!--========== SEARCH BAR ENDS ==========-->
                    <!--========== CONTROLLERS NAV STARTS ==========-->
                    <div class="col-lg-6 col-md-7 col-sm-4 col-xs-4 header-midle flex logo_header">
                        <div class="flex center logo delay05">
                            <a href="https://minim-demo.myshopify.com" class="left">
