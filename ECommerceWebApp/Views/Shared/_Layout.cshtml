                            </a>
                        </div>
                        <div class="hidden-sm hidden-xs flex center menu-header-v2">
                            <ul class="nav navbar-nav menu-main">
                                <li class="relative dropdown">
                                    <a asp-controller="Home" asp-action="Index">Home</a>
                                </li>
                                <li class="relative dropdown">
                                    <a asp-controller="Products" asp-action="Index">Products</a>
                                </li>
                                <li class="relative dropdown">
                                    <a asp-controller="Categories" asp-action="Index">Categories</a>
                                </li>
                                @if (isAdmin)
                                {
                                    <li class="relative dropdown">
                                        <a class="nav-link text-dark" asp-area="" asp-controller="Users" asp-action="Index">Users</a>
                                    </li>
                                }
                            </ul>
                        </div>
                    </div>
                    <!--========== CONTROLLERS NAV ENDS ==========-->
                    <!--========== ICONS STARTS ==========-->
                    <div class="col-lg-3 col-md-3 col-sm-4 col-xs-4 text-right icon-main">
                        @if (Context.Session.GetString("userImg") != null)
                        {
                            <ul class="nav navbar-nav menu-main">
                                <li class="relative dropdown">
                                    <a asp-controller="Users" asp-action="Account" class="delay03 inline-block hidden-sm hidden-xs">
                                        <img src="@Context.Session.GetString("userImg")" class="img-responsive" width="30" style="border-radius:50%">
                                    </a>
                                    <ul class="dropdown-menu">
                                        <li> <a asp-controller="Users" asp-action="Account" class="delay03 capital"> Details </a> </li>
                                        <li> <a asp-controller="Users" asp-action="Edit" class="delay03 capital"> Edit Details </a> </li>
                                        <li> <a asp-controller="Orders" asp-action="Index" class="delay03 capital"> Orders </a> </li>
                                        <li> <a asp-controller="Users" asp-action="Logout" class="delay03 capital"> Logout </a> </li>
                                    </ul>
                                </li>
                            </ul>
                        }
                        else
                        {
                            <ul class="nav navbar-nav menu-main">
                                <li class="relative dropdown">
                                    <a asp-controller="Users" asp-action="Account" class="delay03 inline-block hidden-sm hidden-xs">
                                        <img src="https://minim-demo.myshopify.com/cdn/shop/t/2/assets/iconlogin.png?v=17151735571898987051552621233" class="img-responsive">
                                    </a>
                                    <ul class="dropdown-menu">
                                        <li> <a asp-controller="Users" asp-action="Login" class="delay03 capital"> Login </a> </li>
                                        <li> <a asp-controller="Users" asp-action="Create" class="delay03 capital"> Register </a> </li>
                                    </ul>
                                </li>
                            </ul>
                        }
                        <ul class="nav navbar-nav menu-main">
                            <li class="relative dropdown">
                                <a asp-controller="Carts" asp-action="Index" class="delay03 relative inline-block text-center">
                                    <img src="https://minim-demo.myshopify.com/cdn/shop/t/2/assets/iconcar.png?v=19912236354837126781552620850" class="img-responsive">
                                </a>
                            </li>
                        </ul>
                    </div>
                    <!--========== ICONS ENDS ==========-->
                </div>
            </div>
        </header>
    </div>

    <main>
        @RenderBody()
    </main>

